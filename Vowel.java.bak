class Vowel 
{
	public static void main(String[] args) 
	{
		char n='@';
		if(n>='a'&&n<='z'||n>='A'&&n<='Z'){
			if(n=='a'||n=='e'||n=='i'||n=='o'||n=='u'||n=='A'||n=='E'||n=='I'||n=='O'||n=='U'){
				System.out.println("The char " +n+ " is vowel");
		}
            else{
				System.out.println("The char " +n+ " is consonant");
	     }
	    else{
			System.out.println("The char "+n+" is special character");
}
}
}
}