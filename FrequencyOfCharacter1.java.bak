import java.lang.String;
class FrequencyOfCharacter1 
{
	public static void main(String[] args) 
	{
		String str="welcome to the java world";
		int count=0;
			for(int i=0;i<str.length()-1;i++){
				if(str.charAt(i)=='t'){
                    count++;
		}
		System.out.println("Freaquency of character t "+count);
	}
}
}
