class FrequencyOfCharacter 
{
	public static void main(String[] args) 
	{
		string str="welcome to the java world";
		int count=0;
			for(int i=0;i<str.length()-1;i++){
				if(str[i]=='t'){
                    count++;
		}
		System.out.println("Freaquency of character:"+count++);
	}
}
}